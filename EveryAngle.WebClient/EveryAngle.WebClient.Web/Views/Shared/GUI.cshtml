        <script>
            $(function () {
                checkReady(function () { return window.WC.HtmlHelper && window.kendo; }, function () {
                    var data = [
                        { id: 1, name: 'Label 1' },
                        { id: 2, name: 'Label 2' },
                        { id: 3, name: 'Label 3' },
                        { id: 4, name: 'Label 4' },
                        { id: 5, name: 'Label 5' },
                        { id: 6, name: 'Label 6' },
                        { id: 7, name: 'Label 7' },
                        { id: 8, name: 'Label 8' }
                    ];
                    var data2 = [
                        { id: 1, name: 'Label 1 (disabled)' }
                    ];
                    WC.HtmlHelper.DropdownList($('#Dropdown1'), data);
                    WC.HtmlHelper.DropdownList($('#Dropdown11'), data2);
                    WC.HtmlHelper.DropdownList($('#Dropdown2'), data, {
                        open: function (e) {
                            e.sender.list.addClass('k-list-container-light');
                        }
                    });
                    WC.HtmlHelper.DropdownList($('#Dropdown22'), data2, {
                        open: function (e) {
                            e.sender.list.addClass('k-list-container-light');
                        }
                    });
                    WC.HtmlHelper.DropdownList($('#Dropdown3'), data);
                    WC.HtmlHelper.DropdownList($('#Dropdown33'), data2);

                    $('#AutoComplete1').kendoAutoComplete({
                        dataValueField: 'id',
                        dataTextField: 'name',
                        dataSource: { data: data }
                    });
                    $('#AutoComplete11').kendoAutoComplete({
                        dataValueField: 'id',
                        dataTextField: 'name',
                        dataSource: { data: data2 }
                    });
                    $('#AutoComplete2').kendoAutoComplete({
                        dataValueField: 'id',
                        dataTextField: 'name',
                        dataSource: { data: data },
                        open: function (e) {
                            e.sender.list.addClass('k-list-container-light');
                        }
                    });
                    $('#AutoComplete22').kendoAutoComplete({
                        dataValueField: 'id',
                        dataTextField: 'name',
                        dataSource: { data: data2 },
                        open: function (e) {
                            e.sender.list.addClass('k-list-container-light');
                        }
                    });

                    $('#NumericTextBox1').kendoNumericTextBox();
                    $('#NumericTextBox11').kendoNumericTextBox();
                    $('#NumericTextBox2').kendoNumericTextBox();
                    $('#NumericTextBox22').kendoNumericTextBox();

                    $('#DateTimePicker1').kendoCustomDateTimePicker();
                    $('#DateTimePicker11').kendoCustomDateTimePicker();

                    var dataDays = [];
                    for (var i = 0; i <= 5; i++) {
                        dataDays.push({ value: i, text: i + ' days' });
                    }
                    for (var i = 10; i <= 50; i += 5) {
                        dataDays.push({ value: i, text: i + ' days' });
                    }
                    for (var i = 60; i <= 100; i += 10) {
                        dataDays.push({ value: i, text: i + ' days' });
                    }
                    data.sortObject('value', enumHandlers.SORTDIRECTION.DESC);
                    $('#TimeSpanPicker1').kendoTimeSpanPicker({
                        dayPickerOptions: {
                            comboBoxOptions: {
                                dataTextField: 'text',
                                dataValueField: 'value',
                                dataSource: dataDays
                            }
                        }
                    });
                    $('#TimeSpanPicker11').kendoTimeSpanPicker();

                    $('#DatePicker1').kendoDatePicker();
                    $('#DatePicker11').kendoDatePicker();
                    $('#DatePicker2').kendoDatePicker({
                        open: function (e) {
                            e.sender.dateView.div.addClass('k-calendar-container-light');
                        }
                    });
                    $('#DatePicker22').kendoDatePicker({
                        open: function (e) {
                            e.sender.dateView.div.addClass('k-calendar-container-light');
                        }
                    });

                    $('#TimePicker1').kendoTimePicker();
                    $('#TimePicker11').kendoTimePicker();
                    $('#TimePicker2').kendoTimePicker();
                    $('#TimePicker22').kendoTimePicker();

                    $('#ColorPicker1').kendoColorPicker({ value: '#ff0000' });
                    $('#ColorPicker11').kendoColorPicker({ value: '#0000ff' });
                    $('#ColorPicker2').kendoCustomColorPicker({ value: '#ff0000' });
                    $('#ColorPicker22').kendoCustomColorPicker({ value: '#0000ff' });
                });
            });

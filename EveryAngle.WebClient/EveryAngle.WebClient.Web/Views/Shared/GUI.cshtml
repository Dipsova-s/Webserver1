@{
    ViewBag.Title = "GUI";
    Layout = "~/Views/Shared/Master.cshtml";
}
@section scripts
{
    @{
        @Styles.Render("~/content/css/widgets.css")
    }

    <style type="text/css">
        #MainContent {
            overflow: auto;
            box-sizing: border-box;
            padding: 0 20px;
            font-size: 12px;
        }

        hr {
            margin: 20px 0;
        }

        #IconDemo {
            text-align: center;
            overflow: hidden;
        }

        #IconDemo .form-row {
            flex-wrap: wrap;
        }
    </style>

    <script>
        if (@Convert.ToString(BundleTable.EnableOptimizations).ToLower()) {
            WC.Page.LoadSearchPageScripts([]);
        }

        var checkReady = function (check, callback) {
            var fnCheck = setInterval(function () {
                if (check()) {
                    clearInterval(fnCheck);
                    callback();
                }
            }, 100);
        };
        $(function () {
            // customize
            $('#MainContent').css({
                'height': WC.Window.Height - $('#TopBar').height()
            });
            checkReady(function () { return window.progressbarModel; }, function () {
                progressbarModel.EndProgressBar();
            });
        });
    </script>
}
